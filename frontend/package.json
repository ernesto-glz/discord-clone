{
  "name": "dclone-frontend",
  "productName": "Discord Clone",
  "version": "0.1.0",
  "description": "A discord clone created with ReactJS and NestJS.",
  "author": "Ernesto Gonzalez <kik0swd@gmail.com>",
  "license": "MIT",
  "main": "dist/electron/main/index.js",
  "scripts": {
    "dev": "vite --host",
    "dev:web": "BROWSER_DEV=true vite",
    "build": "tsc && vite build && electron-builder",
    "build:web": "tsc && vite build --config vite.config.prod.ts"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "@discord/types": "link:../types/",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "1.8.1",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "date-fns": "2.29.1",
    "discord-markdown": "2.5.1",
    "events": "3.3.0",
    "framer-motion": "6.5.1",
    "react": "18.2.0",
    "react-contextmenu": "2.14.0",
    "react-dom": "18.2.0",
    "react-easy-crop": "^4.4.2",
    "react-hook-form": "7.34.0",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-spinners": "0.11.0",
    "socket.io-client": "4..0",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/throttle-debounce": "5.0.0",
    "@vitejs/plugin-react": "2.0.0",
    "electron": "19.0.9",
    "electron-builder": "23.1.0",
    "prop-types": "15.8.1",
    "sass": "1.54.3",
    "typescript": "4.7.4",
    "vite": "3.0.2",
    "vite-plugin-electron": "0.8.3"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "0.0.0.0",
    "VITE_DEV_SERVER_PORT": 3000
  }
}
